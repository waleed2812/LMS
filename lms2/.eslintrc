{
    "extends": "eslint:recommended",
    "env": {
        "browser": false,
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "rules": {
        "object-curly-spacing": [
            2,
            "always"
        ],
        "strict": 0,
        "quotes": [
            2,
            "single",
            "avoid-escape"
        ],
        "semi": [
            2,
            "always"
        ],
        "space-before-function-paren": [
            0,
            "ignore"
        ],
        "keyword-spacing": [
            2,
            {
                "before": true,
                "after": true
            }
        ],
        "space-infix-ops": 2,
        "spaced-comment": [
            2,
            "always"
        ],
        "arrow-spacing": 2,
        "no-console": 0,
        "no-unused-vars": [
            "error",
            {
                "vars": "all",
                "args": "none",
                "ignoreRestSiblings": false
            }
        ],
        "no-inner-declarations": 0,
        "no-prototype-builtins": 0,
        "no-useless-catch": 0,
        "for-direction": 0,
        "getter-return": 0,
        "no-misleading-character-class": 0,
        "no-useless-escape": 0,
        "no-async-promise-executor": 0
    },
    "globals": {
        "config": true,
        "next": true,
        "angular": true,
        "io": true,
        "logEvent": true,
        "$": true,
        "document": true,
        "window": true,
        "location": true,
        "FormData": true,
        "Cookies": true,
        "screen": true,
        "google": true,
        "marker": true,
        "map": true,
        "navigator": true,
        "moment": true,
        "fbq": true,
        "isUserData": true,
        "localStorage": true,
        "ENVIRONMENT": true,
        "gtag": true
    }
}